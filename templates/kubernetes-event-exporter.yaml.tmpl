config:
  logLevel: debug
  logFormat: json
  route:
    routes:
      - match:
        - receiver: "dump"
        - receiver: "loki"
      - drop:
        - namespace: "monitoring"
    receivers:
      - name: "dump"
        stdout: {}
      - name: "loki"
        webhook:
          endpoint: "${loki_gateway_monitoring_url}/loki/api/v1/push"
          headers:
            User-Agent: kube-event-exporter 1.0
